// get the declarations
#include "meta.h"


// version info
auto
flocor::meta::
version() -> version_t
{
    return version_t {
        FLOCOR_major, FLOCOR_minor, FLOCOR_micro,
        FLOCOR_revision
    };
}


// compilation date
auto
flocor::meta::
date() -> string_t
{
    return FLOCOR_date;
}


