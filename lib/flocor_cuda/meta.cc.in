// get the declarations
#include "meta.h"


// version info
auto
flocor::cuda::meta::
version() -> version_t
{
    return version_t {
        FLOCOR_CUDA_major, FLOCOR_CUDA_minor, FLOCOR_CUDA_micro,
        FLOCOR_CUDA_revision
    };
}


// compilation date
auto
flocor::cuda::meta::
date() -> string_t
{
    return FLOCOR_CUDA_date;
}


